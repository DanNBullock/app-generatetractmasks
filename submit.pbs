#!/bin/bash
#PBS -l nodes=1:ppn=16:dc2,walltime=0:45:00
#PBS -N app-generatetractmasks
#PBS -V

cd $PBS_O_WORKDIR

module load matlab

export MATLABPATH=$MATLABPATH:$SCA_SERVICE_DIR
matlab -nodisplay -nosplash -r main

if [ -d "masks" ];
then 
  echo 0 > finished
else
  echo "directory missing"
  echo 1 > finished
  exit 1
fi
